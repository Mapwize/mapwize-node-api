{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": ".",
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true,
        "readme": "README.md"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "docdash": {
        "search": true,
        "menu": {
            "Github repo": {
                "href":"https://github.com/Mapwize/mapwize-node-api",
                "target":"_blank",
                "class":"menu-item",
                "id":"website_link"
            },
            "Mapwize": {
                "href":"https://www.mapwize.io",
                "target":"_blank",
                "class":"menu-item",
                "id":"website_link"
            }
        }
    }
}